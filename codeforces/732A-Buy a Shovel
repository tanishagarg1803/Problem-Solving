// Problem Link: https://codeforces.com/contest/732/problem/A

#include<iostream>
using namespace std;
int main(){
    int c,k,n=10;
    cin>>c>>k;
    for(int i=1;i<=9;i++){
        if(((c*i)%10)==0){
            n=i;
            break;
        }
        else if(((c*i)-k)%10==0){
            n=i;
            break;
        }
    }
    cout<<n;
}
